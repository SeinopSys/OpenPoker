{
  "name": "@seinopsys/openpoker",
  "version": "0.1.0",
  "description": "Helps create and run a SCRUM planning poker session for estimations",
  "author": "SeinopSys <david@seinopsys.dev> (https://seinopsys.dev)",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:server": "nest build",
    "build:client": "cd src/client && next build",
    "build": "concurrently -p name -g \"npm:build:*\"",
    "format": "prettier --write \"./**/*.ts\" \"./**/*.tsx\"",
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node ./prod-start.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --randomize",
    "test:watch": "jest --randomize --watch",
    "test:cov": "jest --randomize --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pretypeorm": "npm run build:server",
    "typeorm": "typeorm-ts-node-commonjs",
    "pretypeorm:generate": "npm run build:server",
    "typeorm:generate": "typeorm-ts-node-commonjs migration:generate -d src/server/common/data-source.ts",
    "pretypeorm:run": "npm run build:server",
    "typeorm:run": "typeorm-ts-node-commonjs migration:run -d src/server/common/data-source.ts",
    "pretypeorm:revert": "npm run build:server",
    "typeorm:revert": "typeorm-ts-node-commonjs migration:revert -d src/server/common/data-source.ts"
  },
  "dependencies": {
    "@eropple/nestjs-correlation-id": "^1.0.1",
    "@js-temporal/polyfill": "^0.4.4",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/platform-socket.io": "^10.3.9",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/throttler": "^5.2.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.9",
    "@octokit/auth-oauth-app": "^8.1.1",
    "@octokit/rest": "^21.0.0",
    "@root/passphrase": "^1.1.0",
    "@vvo/tzdb": "^6.140.0",
    "axios": "^1.7.2",
    "base64-arraybuffer": "^1.0.2",
    "bull": "^4.14.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^7.1.1",
    "csrf-sync": "^4.0.3",
    "express-session": "^1.18.0",
    "express-socket.io-session": "^1.3.5",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "nestjs-pino": "^4.1.0",
    "next": "^14.2.4",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0",
    "pg": "^8.12.0",
    "pino-pretty": "^11.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-topbar-progress-indicator": "^4.1.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "shell-escape": "^0.2.0",
    "socket.io-react-hook": "^2.4.4",
    "sodium-native": "^4.1.1",
    "swr": "^2.2.5",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@next/eslint-plugin-next": "^14.2.4",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/express-socket.io-session": "^1.3.9",
    "@types/jest": "29.5.12",
    "@types/node": "20.14.9",
    "@types/passport-oauth2": "^1.4.17",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/shell-escape": "^0.2.3",
    "@types/sodium-native": "^2.3.9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.52.0",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "29.7.0",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "overrides": {
    "@liaoliaots/nestjs-redis": {
      "@nestjs/common": "$@nestjs/common",
      "@nestjs/core": "$@nestjs/core"
    },
    "@liaoliaots/nestjs-redis-health": {
      "@nestjs/common": "$@nestjs/common",
      "@nestjs/core": "$@nestjs/core",
      "@nestjs/terminus": "^10.0.0"
    }
  }
}
