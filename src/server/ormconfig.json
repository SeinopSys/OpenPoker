{
  "name": "default",
  "type": "postgres",
  "host": "127.0.0.1",
  "port": 5432,
  "username": "openpoker",
  "password": "",
  "database": "openpoker",
  "entities": [
    "dist/**/*.entity.js"
  ],
  "uuidExtension": "pgcrypto",
  "logging": true,
  "logger": "advanced-console",
  "synchronize": false,
  "migrations": [
    "dist/**/migrations/*.js"
  ],
  "migrationsRun": true,
  "ssl": true,
  "cli": {
    "migrationsDir": "src/server/migrations"
  },
  "extra": {
    "ssl": {
      "rejectUnauthorized": false
    }
  }
}

